<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sushil.androidmaterialtest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/plant"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/main_activity_label_name">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sushil.androidmaterialtest.LoginActivity" />
        </activity>

        <activity
            android:name=".SecondaryActivity"
            android:icon="@drawable/ic_launcher1"
            android:label="@string/color_extraction">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sushil.androidmaterialtest.MainActivity" />
        </activity>

        <activity
            android:name=".SplashScreen"
            android:label="RNA">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginActivity"
            android:icon="@drawable/ic_launcher1"
            android:label="@string/login_activity"></activity>
    </application>

</manifest>
